pipeline{
agent any
stages
{
stage('Build')
{
steps{
echo "Building the code..."
bat "mvn clean"
}
}
stage('Testing')
{
steps{
echo "Testing the code..."
bat "mvn clean"
}
}
stage('Compile')
{
steps{
echo "Compile the code..."
bat "mvn clean"
}
}
stage('Bdeploying')
{
steps{
echo "Deploying the code..."
}
}
}
}
